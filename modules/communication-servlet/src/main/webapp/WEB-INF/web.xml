<web-app>
  <display-name>The PORS Servlet Application</display-name>
  <servlet>
    <servlet-name>PorsServlet</servlet-name>
    <servlet-class>org.openehealth.pors.communication_servlet.PorsServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PorsServlet</servlet-name>
    <url-pattern>/PorsServlet</url-pattern>
  </servlet-mapping>
</web-app>